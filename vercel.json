{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin-allow-popups"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health.js"
    },
    {
      "source": "/api/ai/analyze",
      "destination": "/api/analyze.js"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/"
    }
  ]
}
